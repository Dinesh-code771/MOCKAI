{
  "name": "backend",
  "version": "1.0.0",
  "description": "mockAI's BackEnd",
  "author": "Sai",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --config jest.config.ts",
    "test:watch": "jest --config jest.config.ts --watch",
    "test:coverage": "jest --config jest.config.ts --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "NODE_ENV=development jest --config jest.e2e.config.ts",
    "test:e2e-coverage": "NODE_ENV=development jest --config jest.e2e.config.ts --coverage",
    "allow:permission": "sudo chmod -R 777 ./data",
    "db:dev:rm": "docker-compose down --volumes --remove-orphans",
    "db:dev:up": "docker-compose up -d",
    "generate:sdk": "npm install @openapitools/openapi-generator-cli -g && openapi-generator-cli generate -i http://localhost:5000/api-json --generator-name typescript-fetch --additional-properties modelPropertyNaming=original -o ../../libs/client_sdk -c sdk-config.json && cp sdk-tsconfig.json ../../libs/client_sdk/tsconfig.json && cd ../../libs/client_sdk && npm run build",
    "generate:client": "rm -rf ../../libs/client_sdk && npm run generate:sdk",
    "generate:openapi-spec": "curl http://localhost:5000/api-json -o openapi.json",
    "prisma:dev:deploy": "npx prisma migrate deploy --schema=./src/db/prisma/schema.prisma",
    "prisma:local:up": "npm run prisma:dev:deploy && npm run prisma:dbpull && npm run prisma:generate",
    "prisma:migrate": "npx prisma migrate dev --schema=./src/db/prisma/schema.prisma",
    "prisma:status": "npx prisma migrate status --schema=./src/db/prisma/schema.prisma",
    "prisma:new-migration": "npx prisma migrate dev --schema=./src/db/prisma/schema.prisma --create-only",
    "prisma:reset": "npx prisma migrate reset --schema=./src/db/prisma/schema.prisma --force",
    "prisma:dbpull": "npx prisma db pull --schema=./src/db/prisma/schema.prisma",
    "prisma:studio": "npx dotenv-cli -e .env -- npx prisma studio --schema=./src/db/prisma/schema.prisma",
    "prisma:generate": "npx prisma generate --schema=./src/db/prisma/schema.prisma",
    "prisma:setup": "npm run prisma:migrate && npm run prisma:dbpull && npm run prisma:generate",
    "prisma:migrate-dbpull": "npm run prisma:migrate && npm run prisma:dbpull",
    "local:up": "npm run db:dev:up && sleep 5 && npm run prisma:setup && npm run start:dev",
    "local:dev:up": "npm run db:dev:up && sleep 5 && npm run prisma:reset && npm run prisma:local:up && npm run start:dev"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.787.0",
    "@aws-sdk/s3-request-presigner": "^3.787.0",
    "@bull-board/api": "^6.9.6",
    "@bull-board/express": "^6.9.6",
    "@bull-board/nestjs": "^6.9.6",
    "@nestjs/axios": "^3.1.0",
    "@nestjs/bullmq": "^10.2.1",
    "@nestjs/cache-manager": "^2.2.2",
    "@nestjs/common": "^10.4.6",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.4.6",
    "@nestjs/devtools-integration": "^0.1.6",
    "@nestjs/event-emitter": "^2.0.4",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.4.6",
    "@nestjs/platform-socket.io": "^10.4.15",
    "@nestjs/schedule": "^4.1.1",
    "@nestjs/serve-static": "^5.0.3",
    "@nestjs/swagger": "^7.4.2",
    "@nestjs/terminus": "^10.2.3",
    "@nestjs/throttler": "^6.2.1",
    "@prisma/client": "^5.21.1",
    "@sendgrid/mail": "^8.1.4",
    "axios": "^1.7.7",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.3",
    "bullmq": "^5.21.2",
    "cache-manager": "^5.7.6",
    "cache-manager-redis-yet": "^5.1.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cookie-parser": "^1.4.7",
    "fs-extra": "^11.2.0",
    "helmet": "^8.0.0",
    "ioredis": "^5.4.1",
    "joi": "^17.13.3",
    "mammoth": "^1.9.0",
    "nest-winston": "^1.9.7",
